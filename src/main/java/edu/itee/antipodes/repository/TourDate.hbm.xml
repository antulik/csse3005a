<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping package="edu.itee.antipodes.domain">
  <class name="TourDate">
  	<id name="dateID" type="integer" column="DATE_ID">
  		<generator class="increment"/> 
  	</id> 	
  	
  	<property name="startDate" column="START_DATE"></property>
  	
  	<property name="finishDate" column="FINISH_DATE"></property>
  	<many-to-one name="tour" class="Tour" column="TOUR_ID" cascade="all" not-null="true"/>
 
 	</class>
</hibernate-mapping>
 
