<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="edu.itee.antipodes.domain.db">
  <class name="MonthlyFee" table="MONTHLYFEE" lazy="false">
  	<id name="feeID" type="integer" column="FEE_ID">
  		<generator class="increment"/>
  	</id>
  	<property name="month" column="MONTH" type="integer"/>
  	<property name="fee" column="FEE" type="float" />
  	<property name="year" column="YEAR" type="integer" />
  </class>
</hibernate-mapping>
